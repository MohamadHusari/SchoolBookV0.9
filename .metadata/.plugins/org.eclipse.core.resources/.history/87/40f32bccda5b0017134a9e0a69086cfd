package Entities;

public class Student {

	private int id;
	private String name;
	public Student(int ID,String Name){
		setId(ID);
		setName(Name);
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
}
